{
  "name": "soluble/expressive-japha",
  "description": "soluble-japha integration for zend-expressive",
  "license": "MIT",
  "keywords": [
    "expressive",
    "zend-expressive",
    "module",
    "soluble-japha",
    "java",
    "bridge"
  ],
  "homepage": "http://soluble.io",
  "type": "library",
  "authors": [
    {
      "name": "SÃ©bastien Vanvelthem",
      "homepage": "https://github.com/belgattitude"
    }
  ],
  "autoload": {
    "psr-4": {
      "Soluble\\Expressive\\Japha\\": "src/Soluble/Expressive/Japha"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SolubleTest\\Expressive\\Japha\\": "tests/SolubleTest/Expressive/Japha"
    }
  },
  "require": {
    "php": "^7.1",
    "soluble/japha": "^2.2.3",
    "zendframework/zend-expressive": "^2.0.0"

  },
  "require-dev" : {
    "phpunit/phpunit": "^6.1.4",
    "friendsofphp/php-cs-fixer": "^2.4.0",
    "pds/skeleton": "@dev",
    "phpstan/phpstan": "^0.8.3",
    "zendframework/zend-config-aggregator": "^1.0.0"

  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "zf": {
      "component": "Soluble\\Expressive\\Japha",
      "config-provider": "Soluble\\Expressive\\Japha\\ConfigProvider"
    }
  },
  "scripts": {
    "check": [
      "@phpstan",
      "@phpds",
      "@cs-check",
      "@test"
    ],
    "fix": [
      "@cs-fix"
    ],
    "test": "vendor/bin/phpunit --colors=always",
    "cs-check": "vendor/bin/php-cs-fixer --diff --dry-run -v fix",
    "cs-fix": "vendor/bin/php-cs-fixer -v fix",
    "phpstan": "vendor/bin/phpstan analyse -l 5 -c phpstan.neon src tests",
    "phpds": "vendor/bin/pds-skeleton validate"
  },
  "suggest": {
    "monolog/monolog": "PSR-3 compatible logger"
  },
  "archive": {
    "exclude": ["tests", "docs", ".travis", ".travis.yml", ".codeclimate.yml", ".coveralls.yml", ".scrutinizer.yml", ".php_cs", ".gitignore", "phpcs.xml"]
  }
}
